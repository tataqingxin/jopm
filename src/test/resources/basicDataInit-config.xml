<?xml version="1.0" encoding="UTF-8"?>
<basic>
	<!-- 1、数据库字典初始化 -->
	<dics>
		<dic typeGroupCode="iconType" typeGroupName="图标类型" >
			<items>
				<item typeName="应用图标" typeCode="1"></item>
				<item typeName="功能图标" typeCode="2"></item>
				<item typeName="操作图标" typeCode="3"></item>
			</items>
		</dic>
		<dic typeGroupCode="include" typeGroupName="是否包含" >
			<items>
				<item typeName="是" typeCode="1"></item>
				<item typeName="否" typeCode="0"></item>
			</items>
		</dic>
		<dic typeGroupCode="appType" typeGroupName="应用类型" >
			<items>
				<item typeName="系统应用" typeCode="1"></item>
				<item typeName="普通应用" typeCode="2"></item>
			</items>
		</dic>
		<dic typeGroupCode="roleType" typeGroupName="角色类型" >
			<items>
				<item typeName="系统角色" typeCode="1"></item>
				<item typeName="普通角色" typeCode="2"></item>
			</items>
		</dic>
		<dic typeGroupCode="yesorno" typeGroupName="逻辑条件" >
			<items>
				<item typeName="是" typeCode="Y"></item>
				<item typeName="否" typeCode="N"></item>
			</items>
		</dic>
		<dic typeGroupCode="sex" typeGroupName="性别类" >
			<items>
				<item typeName="女性" typeCode="0"></item>
				<item typeName="男性" typeCode="1"></item>
			</items>
		</dic>
		<dic typeGroupCode="status" typeGroupName="启用停用状态" >
			<items>
				<item typeName="停用" typeCode="0"></item>
				<item typeName="启用" typeCode="1"></item>
			</items>
		</dic>
	</dics>
